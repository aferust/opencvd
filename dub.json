{
    "copyFiles-posix": [
        "c/build/*.a",
        "c/build/contrib/*.a"
    ],
    "copyFiles-windows": [
        "c/build/Debug/*.lib",
        "c/build/contrib/Debug/*.lib",
        "c/build/Debug/*.pdb",
        "c/build/contrib/Debug/*.pdb"
    ],
    "description": "OpenCV Wrapper for dlang",
    "license": "Boost",
    "authors": [
        "Ferhat Kurtulmuş"
    ],
    "copyright": "Copyright © 2019, Ferhat Kurtulmuş",
    "name": "opencvd",
    "version": "0.0.6",
    "importPaths": ["source"],
    "sourcePaths": ["source"],
    "dflags-windows": ["-mtriple=x86_64-windows-msvc"],
    "libs-posix": ["opencv4", "opencvcapi", "opencvcapi_contrib"],
    "lflags-posix": ["-lstdc++"],
    "libs-windows": [
        "opencv_world430",
        "opencv_img_hash430",
        "opencvcapi",
        "opencvcapi_contrib"
    ],
    "configurations": [
        {
            "name": "library",
            "targetType": "library",
            "sourcePaths": ["source"]
        },
        {
            "name": "import",
            "targetType": "sourceLibrary",
            "sourceFiles-posix": ["libopencvd.a"],
            "sourceFiles-windows": ["opencvd.lib"]
        }

    ]
}
